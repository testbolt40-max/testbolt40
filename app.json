{
  "expo": {
    "name": "RideShare App",
    "slug": "rideshare-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rideshare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "A modern ride-sharing application with real-time tracking and booking features",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rideshare.app"
    },
    "android": {
      "package": "com.rideshare.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "black"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      "expo-location",
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_SUPABASE_URL": "https://your-project-url.supabase.co",
      "EXPO_PUBLIC_SUPABASE_ANON_KEY": "your-anon-key",
      "EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY": "pk_test_your_stripe_publishable_key_here",
      "EXPO_PUBLIC_API_URL": "https://your-api-url.com"
    }
  }
}
